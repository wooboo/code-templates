{
    "name": "code-templates",
    "displayName": "code-templates",
    "description": "file and folder templates for VS Code",
    "version": "0.0.1",
    "publisher": "piotrzabowka",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createFromTemplate"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createFromTemplate",
                "title": "Create from template"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.createFromTemplate",
                    "group": "templates"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.createFromTemplate",
                    "group": "templates"
                }
            ]
        },
        "keybindings": [
            {
                "command": "extension.createFromTemplate",
                "key": "ctrl+n",
                "mac": "cmd+n"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}